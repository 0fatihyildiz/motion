{
  "name": "@vueuse/motion",
  "version": "2.0.0-beta.18",
  "description": "ðŸ¤¹ Vue Composables putting your components in motion",
  "repository": "https://github.com/vueuse/motion",
  "bugs": {
    "url": "https://github.com/vueuse/motion/issues"
  },
  "homepage": "https://github.com/vueuse/motion#readme",
  "author": "YaeÌˆl GUILLOUX <yael.guilloux@gmail.com>",
  "license": "MIT",
  "keywords": [
    "vue",
    "hook",
    "motion",
    "animation",
    "v-motion",
    "popmotion-vue"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./nuxt": {
      "import": "./dist/nuxt.mjs",
      "require": "./dist/nuxt.cjs"
    }
  },
  "scripts": {
    "build": "unbuild",
    "dev": "jiti scripts/watch.ts --cache",
    "lint": "eslint --ext .js,.vue,.ts,.tsx .",
    "lint:fix": "eslint --ext .js,.vue,.ts,.tsx . --fix",
    "test:types": "tsc --build tsconfig.json",
    "test:unit": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test": "pnpm test:types && pnpm test:unit && pnpm test:coverage",
    "dev:docs": "nuxt dev docs",
    "build:docs": "nuxt generate docs",
    "preview:docs": "nuxt preview docs",
    "publish:ci": "jiti scripts/publish.ts",
    "release": "jiti scripts/release.ts && git push --follow-tags",
    "dev:demo": "vite",
    "build:demo": "vite build",
    "serve:demo": "vite serve demo/dist",
    "prepare:nuxt": "nuxi prepare playground && unbuild --stub",
    "build:nuxt": "nuxt-module-build",
    "dev:nuxt": "nuxi dev nuxt-playground",
    "build:nuxt:playground": "nuxi build playground",
    "prepare": "pnpm run prepare:nuxt"
  },
  "files": [
    "dist/**/*",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "@vueuse/core": "^8.9.4",
    "@vueuse/shared": "^8.9.4",
    "csstype": "^3.1.0",
    "framesync": "^6.1.0",
    "popmotion": "^11.0.3",
    "style-value-types": "^5.1.0"
  },
  "peerDependencies": {
    "@nuxt/kit": "^3.0.0-rc.4"
  },
  "devDependencies": {
    "@nuxt-themes/docus": "npm:@nuxt-themes/docus-edge@latest",
    "@nuxt/module-builder": "latest",
    "@types/prismjs": "^1.26.0",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vue/server-renderer": "^3.2.37",
    "@vue/test-utils": "^2.0.2",
    "bumpp": "^8.2.1",
    "c8": "^7.12.0",
    "chokidar": "^3.5.3",
    "happy-dom": "^6.0.4",
    "lint-staged": "^13.0.3",
    "nuxt": "^3.0.0-rc.4",
    "prism-theme-vars": "^0.2.4",
    "prismjs": "^1.28.0",
    "typescript": "^4.7.4",
    "unbuild": "^0.7.6",
    "vite": "3.0.2",
    "vite-plugin-windicss": "^1.8.7",
    "vitepress": "^0.22.4",
    "vitest": "^0.18.1",
    "vue": "^3.2.37",
    "yorkie": "^2.0.0",
    "eslint": "^8.20.0",
    "@antfu/eslint-config": "^0.25.2",
    "@nuxtjs/eslint-config-typescript": "^10.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-prettier-vue": "^4.2.0",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.12"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ],
    "*.ts?(x)": [
      "eslint --fix"
    ]
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@types/react",
        "react",
        "react-dom"
      ]
    }
  }
}
