{
  "name": "@vueuse/motion",
  "type": "module",
  "version": "2.0.0",
  "description": "ðŸ¤¹ Vue Composables putting your components in motion",
  "author": "YaeÌˆl GUILLOUX <yael.guilloux@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/vueuse/motion#readme",
  "repository": "https://github.com/vueuse/motion",
  "bugs": {
    "url": "https://github.com/vueuse/motion/issues"
  },
  "keywords": [
    "vue",
    "hook",
    "motion",
    "animation",
    "v-motion",
    "popmotion-vue"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./plugin": {
      "types": "./dist/plugin.d.ts",
      "require": "./dist/plugin.cjs",
      "import": "./dist/plugin.mjs"
    },
    "./presets": {
      "types": "./dist/presets.d.ts",
      "require": "./dist/presets.cjs",
      "import": "./dist/presets.mjs"
    },
    "./nuxt": {
      "types": "./dist/nuxt.d.ts",
      "require": "./dist/nuxt.cjs",
      "import": "./dist/nuxt.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub",
    "dev": "pnpm dev:vite",
    "release": "release-it",
    "lint": "eslint --ext .js,.vue,.ts,.tsx .",
    "lint:fix": "eslint --ext .js,.vue,.ts,.tsx . --fix",
    "test:unit": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test": "pnpm test:unit && pnpm test:coverage",
    "prepare": "pnpm run stub && pnpm -r --filter motion-* run stub",
    "__": "__",
    "dev:nuxt": "pnpm --filter motion-nuxt-example dev"
  },
  "peerDependencies": {
    "vue": ">=3.0.0"
  },
  "dependencies": {
    "@vueuse/core": "^10.1.2",
    "@vueuse/shared": "^10.1.2",
    "csstype": "^3.1.2",
    "framesync": "^6.1.2",
    "popmotion": "^11.0.5",
    "style-value-types": "^5.1.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.5",
    "@nuxt/kit": "^3.5.3",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@vitest/coverage-c8": "^0.32.0",
    "@vue/test-utils": "^2.3.2",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-prettier-vue": "^4.2.0",
    "happy-dom": "^9.20.3",
    "prettier": "^2.8.8",
    "simple-git-hooks": "^2.8.1",
    "typescript": "^5.1.3",
    "unbuild": "^1.2.1",
    "vite": "^4.3.9",
    "vitest": "^0.32.0",
    "vue": "^3.3.4"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue}": [
      "eslint --cache --fix"
    ]
  },
  "release-it": {
    "hooks": {
      "before:init": [
        "pnpm build"
      ]
    },
    "npm": {
      "access": "public"
    },
    "git": {
      "commitMessage": "chore(release): release v${version}"
    },
    "github": {
      "release": true,
      "releaseName": "v${version}"
    }
  }
}
